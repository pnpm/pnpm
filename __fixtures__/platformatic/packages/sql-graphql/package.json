  },
  "homepage": "https://github.com/platformatic/platformatic#readme",
  "devDependencies": {
    "@mercuriusjs/gateway": "^1.2.0",
    "@platformatic/sql-events": "workspace:*",
    "@platformatic/sql-mapper": "workspace:*",
    "fastify": "^4.13.0",
    "snazzy": "^9.0.0",
    "standard": "^17.0.0",
    "tap": "^16.3.4",
    "tsd": "^0.28.0",
    "ws": "^8.12.1"
  },
  "dependencies": {
    "@mercuriusjs/federation": "^1.0.1",
    "camelcase": "^6.3.0",
    "fastify-plugin": "^4.5.0",
