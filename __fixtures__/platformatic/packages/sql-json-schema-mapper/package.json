  },
  "homepage": "https://github.com/platformatic/platformatic#readme",
  "devDependencies": {
    "@platformatic/sql-mapper": "workspace:*",
    "fastify": "^4.13.0",
    "snazzy": "^9.0.0",
    "standard": "^17.0.0",
    "tap": "^16.3.4"
  }
