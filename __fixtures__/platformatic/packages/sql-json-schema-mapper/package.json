    "test:mariadb": "DB=mariadb tap test/*.test.js",
    "test:mysql": "DB=mysql tap test/*.test.js",
    "test:mysql8": "DB=mysql8 tap test/*.test.js",
    "test:sqlite": "DB=sqlite tap test/*.test.js"
  },
