{
  "name": "@pnpm/hooks.read-package-hook",
  "version": "3.0.10",
  "description": "Creates the default package reader hook used by pnpm",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "!*.map"
  ],
  "scripts": {
    "lint": "eslint \"src/**/*.ts\" \"test/**/*.ts\"",
