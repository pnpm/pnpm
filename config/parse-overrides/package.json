  "files": [
    "lib",
    "!*.map"
  ],
  "keywords": [
    "pnpm8"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=16.14"
  },
  "repository": "https://github.com/pnpm/pnpm/blob/main/config/parse-overrides",
  "scripts": {
