    "@pnpm/core-loggers": "workspace:*",
    "@pnpm/deps.graph-sequencer": "workspace:*",
    "@pnpm/fs.hard-link-dir": "workspace:*",
    "@pnpm/lifecycle": "workspace:*",
    "@pnpm/link-bins": "workspace:*",
    "@pnpm/patching.apply-patch": "workspace:*",
