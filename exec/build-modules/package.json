    "@pnpm/types": "workspace:*",
    "@pnpm/worker": "workspace:*",
    "p-defer": "^3.0.0",
    "ramda": "npm:@pnpm/ramda@0.28.1",
    "run-groups": "^3.0.1"
  },
  "devDependencies": {
    "@pnpm/build-modules": "workspace:*",
    "@types/ramda": "0.29.12"
  },
