    "compile": "tsc --build && pnpm run lint --fix",
    "_test": "jest"
  },
  "repository": "https://github.com/pnpm/pnpm/blob/main/exec/build-modules",
  "keywords": [
    "pnpm9",
    "pnpm",
    "resolver",
    "npm"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pnpm/pnpm/issues"
  },
