{
  "name": "@pnpm/build-modules",
  "version": "12.0.1",
  "description": "Build packages in node_modules",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "!*.map"
  ],
  "engines": {
    "node": ">=18.12"
  },
