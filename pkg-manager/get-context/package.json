  "description": "Gets context information about a project",
  "keywords": [
    "pnpm",
    "pnpm10",
    "scripts"
  ],
  "license": "MIT",
  "funding": "https://opencollective.com/pnpm",
  "repository": "https://github.com/pnpm/pnpm/blob/main/pkg-manager/get-context",
