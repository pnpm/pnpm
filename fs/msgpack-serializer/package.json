{
  "name": "@pnpm/msgpack-serializer",
  "version": "1000.0.0",
  "description": "Msgpack serialization utilities for pnpm cache files",
  "keywords": [
    "pnpm",
    "pnpm11"
  ],
  "license": "MIT",
  "funding": "https://opencollective.com/pnpm",
  "repository": "https://github.com/pnpm/pnpm/tree/main/fs/msgpack-serializer",
  "homepage": "https://github.com/pnpm/pnpm/tree/main/fs/msgpack-serializer#readme",
  "bugs": {
    "url": "https://github.com/pnpm/pnpm/issues"
  },
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": "./lib/index.js"
  },
  "files": [
    "lib",
    "!*.map"
  ],
  "scripts": {
    "compile": "tsgo --build && pnpm run lint --fix",
    "watch": "tsgo --build --watch",
    "prepublishOnly": "pnpm run compile",
    "test": "pnpm run compile && pnpm run _test",
    "lint": "eslint \"src/**/*.ts\" \"test/**/*.ts\"",
    "_test": "cross-env NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" jest"
  },
  "dependencies": {
    "msgpackr": "catalog:"
  },
  "devDependencies": {
    "@pnpm/msgpack-serializer": "workspace:*",
    "tempy": "catalog:"
  },
  "engines": {
    "node": ">=20.19"
  },
  "jest": {
    "preset": "@pnpm/jest-config"
  }
}
