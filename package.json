{
  "name": "pnpm",
  "description": "A fast implementation of npm install",
  "version": "0.48.1",
  "author": "Rico Sta. Cruz <rico@ricostacruz.com>",
  "bin": {
    "pnpm": "lib/bin/pnpm.js"
  },
  "bugs": {
    "url": "http://ghub.io/pnpm/issues"
  },
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib/"
  ],
  "dependencies": {
    "@types/byline": "4.2.31",
    "@types/cache-manager": "1.2.4",
    "@types/chalk": "0.4.31",
    "@types/common-tags": "1.2.5",
    "@types/debug": "0.0.29",
