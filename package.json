    "cz-conventional-changelog": "^2.0.0",
    "deep-require-cwd": "^1.0.0",
    "exists-link": "^2.0.0",
    "husky": "^0.13.2",
    "in-publish": "^2.0.0",
    "isexe": "^2.0.0",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.0.1",
    "npm-scripts-info": "^0.3.6",
    "pnpm-registry-mock": "^0.12.0",
    "read-pkg": "^2.0.0",
    "rimraf": "^2.5.4",
    "sepia": "^2.0.2",
    "tape": "^4.6.3",
    "tape-promise": "^2.0.1",
    "thenify": "^3.2.1",
