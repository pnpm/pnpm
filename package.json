    "package-preview": "^1.0.0",
    "pnpm-registry-mock": "^1.22.0",
    "rimraf": "^2.6.2",
    "rimraf-then": "^1.0.1",
    "sinon": "^5.0.0",
    "tape": "^4.8.0",
