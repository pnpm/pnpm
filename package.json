{
  "name": "@pnpm/headless",
  "description": "Fast installation using only shrinkwrap.yaml",
  "version": "0.0.0",
  "author": {
    "name": "Zoltan Kochan",
    "email": "z@kochan.io",
    "url": "https://www.kochan.io/"
  },
  "bugs": {
    "url": "https://github.com/pnpm/headless/issues"
  },
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib/"
  ],
  "devDependencies": {
    "@commitlint/cli": "^6.1.3",
    "@commitlint/config-conventional": "^6.1.3",
    "@commitlint/prompt-cli": "^6.1.3",
    "@types/tape": "^4.2.30",
    "@zkochan/husky": "^0.0.0",
    "npm-run-all": "^4.0.1",
    "package-preview": "^1.0.0",
    "pnpm-registry-mock": "^1.17.0",
    "rimraf": "^2.6.2",
    "tape": "^4.8.0",
    "tape-promise": "^2.0.1",
    "ts-node": "^5.0.1",
    "tslint": "^5.4.2",
    "typescript": "^2.7.1"
  },
  "directories": {
    "test": "test"
  },
  "homepage": "https://github.com/pnpm/headless#readme",
  "keywords": [
    "pnpm",
    "install",
    "installer"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pnpm/headless.git"
  },
  "scripts": {
    "start": "npm run tsc -- --watch",
    "commit": "commit",
    "commitmsg": "commitlint -e",
    "lint": "tslint -c tslint.json --project .",
    "lint-test": "tslint -c tslint.json --project test",
    "pnpm-registry-mock": "pnpm-registry-mock",
    "test:tap": "ts-node test",
    "test:e2e": "npm-run-all -p -r pnpm-registry-mock test:tap",
    "pretest": "rimraf ../.tmp/ && pnpm-registry-mock prepare",
    "test": "npm run lint && npm run lint-test && preview && npm run test:e2e",
    "tsc": "rimraf lib && tsc",
    "prepublishOnly": "npm run tsc"
  }
}
