    "@types/node": "6.0.48",
    "@types/rc": "0.0.1",
    "@types/semver": "5.3.30",
    "@types/update-notifier": "0.0.28",
    "@zkochan/cmd-shim": "2.2.1",
    "@zkochan/logger": "0.1.0",
    "arr-flatten": "1.0.1",
    "byline": "5.0.0",
    "cache-manager": "2.3.0",
    "cache-manager-fs": "1.0.6",
    "camelcase-keys": "4.0.0",
    "chalk": "1.1.3",
    "common-tags": "1.4.0",
    "commondir": "1.0.1",
    "cross-spawn": "^5.0.0",
    "debug": "2.6.0",
    "delocalize-dependencies": "0.1.0",
    "exists-file": "3.0.0",
    "find-up": "2.1.0",
    "is-ci": "1.0.10",
    "is-retry-allowed": "1.1.0",
    "is-windows": "1.0.0",
    "js-yaml": "3.7.0",
    "json2yaml": "1.1.0",
    "link-dir": "1.0.3",
    "load-json-file": "2.0.0",
    "lockfile": "1.0.3",
    "lodash.clonedeep": "4.5.0",
    "lodash.memoize": "4.1.2",
    "loud-rejection": "1.6.0",
    "mem": "1.1.0",
    "meow": "3.7.0",
    "mkdirp": "0.5.1",
    "mz": "2.6.0",
    "node-gyp": "3.5.0",
    "normalize-path": "2.0.1",
    "npm-package-arg": "4.2.0",
    "npm-registry-client": "7.4.5",
    "observatory": "1.0.0",
    "os-homedir": "1.0.2",
    "path-name": "1.0.0",
    "pause-stream": "0.0.11",
    "pnpm-install-checks": "1.0.1",
    "promisequence": "1.1.6",
    "rc": "1.1.6",
    "read-pkg-up": "1.0.1",
    "registry-auth-token": "3.1.0",
    "registry-url": "3.1.0",
    "retry": "0.10.1",
    "rimraf-then": "1.0.1",
    "semver": "5.3.0",
    "sorted-object": "2.0.1",
    "supports-color": "3.2.3",
    "thenify": "3.2.1",
    "unpack-stream": "1.0.2",
    "update-notifier": "1.0.3",
    "write-pkg": "2.0.0"
  },
  "devDependencies": {
    "@types/tape": "4.2.28",
    "anonymous-npm-registry-client": "0.1.2",
    "caw": "2.0.0",
    "commitizen": "^2.8.6",
    "cz-conventional-changelog": "^1.2.0",
    "docpress": "0.7.1",
    "exists-link": "1.0.0",
    "ghooks": "^2.0.0",
    "git-update-ghpages": "1.3.0",
    "in-publish": "2.0.0",
    "isexe": "1.1.2",
    "istanbul": "^0.4.5",
    "ncp": "^2.0.0",
    "nixt": "0.5.0",
    "npm-run-all": "4.0.0",
    "npm-scripts-info": "0.3.6",
    "rimraf": "2.5.4",
    "sepia": "2.0.2",
    "standard": "8.6.0",
    "tap-diff": "0.1.1",
    "tape": "4.6.3",
    "tape-promise": "2.0.1",
    "ts-node": "2.0.0",
    "tslint": "4.3.1",
    "typescript": "2.1.4",
    "validate-commit-msg": "^2.7.0",
    "verdaccio": "2.1.0"
  },
  "directories": {
    "test": "test"
  },
  "homepage": "http://ghub.io/pnpm#readme",
  "keywords": [
    "dependency manager",
    "install",
    "modules",
    "npm",
    "package manager"
  ],
  "license": "MIT",
  "preferGlobal": true,
  "engines": {
    "node": ">=4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rstacruz/pnpm.git"
  },
  "scripts": {
    "info": "npm-scripts-info",
    "start": "npm run tsc -- --watch",
    "?docs:build": "echo 'Generate the documentation website'",
    "docs:build": "docpress build",
    "?commit": "echo 'Run Git commit wizard'",
    "commit": "git-cz",
    "cover": "istanbul cover test/index.js",
    "lint": "standard && tslint -c tslint.json --project .",
    "verdaccio": "verdaccio -c ./verdaccio/config.yaml",
    "prebuild-verdaccio": "rimraf verdaccio/storage/",
    "publish-test-packages": "sh test/packages/publish-all.sh",
    "?build-verdaccio": "echo 'Cache the verdaccio store for faster tests execution'",
    "build-verdaccio": "npm-run-all -p -r verdaccio publish-test-packages",
    "test:tap": "ts-node test | tap-diff",
    "test:e2e": "npm-run-all -p -r verdaccio test:tap",
    "pretest": "rimraf ../.tmp/ && npm run tsc",
    "test": "npm run lint && npm run test:e2e",
    "posttest": "git checkout -- verdaccio/storage/",
    "prerelease": "npm run tsc && npm test",
    "?release": "echo 'Publish pnpm'",
    "release": "sh .scripts/release.sh",
    "tsc": "rimraf lib && tsc",
    "prepublish": "in-publish && echo 'You need to use \"npm run release\" to publish pnpm' && false || npm run tsc"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "commit-msg": "node ./node_modules/validate-commit-msg/index.js"
    }
  }
}
