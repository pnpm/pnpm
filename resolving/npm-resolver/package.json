    "@pnpm/fetch": "workspace:*",
    "@pnpm/npm-resolver": "workspace:*",
    "@pnpm/test-fixtures": "workspace:*",
    "@types/normalize-path": "^3.0.0",
    "@types/ramda": "0.28.20",
    "@types/semver": "7.5.3",
    "@types/ssri": "^7.1.2",
