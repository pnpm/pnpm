  "directories": {
    "test": "test"
  },
  "homepage": "https://pnpm.io",
  "keywords": [
    "pnpm10",
    "dependency manager",
    "install",
    "installer",
    "uninstall",
    "remove",
    "link",
    "prune",
    "shrinkwrap",
    "lockfile",
    "fast",
    "rapid",
    "efficient",
    "package.json",
    "packages",
    "dependencies",
    "symlinks",
    "hardlinks",
    "modules",
    "npm",
    "package manager",
    "monorepo",
    "multi-package",
    "workspace:*"
  ],
  "license": "MIT",
  "preferGlobal": true,
  "engines": {
    "node": ">=18.12"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pnpm/pnpm.git",
    "directory": "pnpm"
  },
  "scripts": {
    "bundle": "ts-node bundle.ts",
    "start": "tsc --watch",
    "lint": "eslint \"src/**/*.ts\" \"test/**/*.ts\"",
    "pretest:e2e": "rimraf node_modules/.bin/pnpm",
    "_test": "cross-env PNPM_REGISTRY_MOCK_PORT=7777 jest",
    "test": "pnpm run compile && pnpm run _test",
    "prepublishOnly": "pnpm compile && publish-packed --prune --npm-client=pnpm --dest=dist",
    "postpublish": "publish-packed",
    "_compile": "tsc --build",
    "compile": "tsc --build && pnpm run lint --fix && rimraf dist bin/nodes && pnpm run bundle && shx cp -r node-gyp-bin dist/node-gyp-bin && shx cp -r node_modules/@pnpm/tabtab/lib/templates dist/templates && shx cp -r node_modules/ps-list/vendor dist/vendor && shx cp pnpmrc dist/pnpmrc"
  },
  "publishConfig": {
    "tag": "next-10",
    "executableFiles": [
      "./dist/node-gyp-bin/node-gyp",
      "./dist/node-gyp-bin/node-gyp.cmd",
      "./dist/node_modules/node-gyp/bin/node-gyp.js"
    ]
  },
  "funding": "https://opencollective.com/pnpm",
  "exports": {
    ".": "./package.json"
  },
  "jest": {
    "preset": "@pnpm/jest-config"
  }
}
