      "semver@<7.5.2": "^7.7.2",
      "send@<0.19.0": "^0.19.0",
      "serve-static@<1.16.0": "^1.16.0",
      "socks@2": "^2.8.1",
      "tmp@<=0.2.3": ">=0.2.4",
      "tough-cookie@<4.1.3": ">=4.1.3",
      "validator@<13.15.22": ">=13.15.22",
      "yaml@<2.2.2": ">=2.2.2"
    }
  },
  "jest": {
    "preset": "@pnpm/jest-config/with-registry"
  },
  "preferGlobal": true,
  "publishConfig": {
    "tag": "next-11",
    "executableFiles": [
      "./dist/node-gyp-bin/node-gyp",
      "./dist/node-gyp-bin/node-gyp.cmd",
      "./dist/node_modules/node-gyp/bin/node-gyp.js"
    ],
    "engines": {
      "runtime": {
        "name": "node",
        "version": "24.11.0",
        "onFail": "download"
      }
    }
  }
}
