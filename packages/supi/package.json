    "test-with-preview": "preview && pnpm run test:e2e",
    "_test": "cross-env PNPM_REGISTRY_MOCK_PORT=4873 pnpm run test:e2e",
    "test": "pnpm run compile && pnpm run _test",
    "prepublishOnly": "pnpm run compile",
