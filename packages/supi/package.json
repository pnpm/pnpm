    "commit": "commit",
    "commitmsg": "commitlint -e",
    "lint": "eslint -c ../../eslint.json src/**/*.ts test/**/*.ts",
    "registry-mock": "registry-mock",
    "test:tap": "cd ../.. && c8 --reporter lcov --reports-dir packages/supi/coverage ts-node packages/supi/test --type-check",
