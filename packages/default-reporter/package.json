    "twitter": "ZoltanKochan"
  },
  "engines": {
    "node": ">=6"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pnpm/pnpm/issues"
  },
  "peerDependencies": {
    "supi": ">=0.24.0 <0.25.0"
  },
  "dependencies": {
    "@pnpm/config": "2.2.0",
    "@pnpm/types": "^1.7.0",
    "@types/common-tags": "^1.2.5",
    "@types/node": "^9.3.0 || 10",
    "@types/ramda": "0.25.34",
    "@types/semver": "^5.4.0",
    "@types/strip-ansi": "^3.0.0",
