{
  "name": "@pnpm/server",
  "version": "3.0.5",
  "description": "A pnpm installer server",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=8.15"
  },
  "scripts": {
