{
  "name": "@pnpm/server",
  "version": "7.0.3",
  "description": "A pnpm installer server",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "lint": "tslint -c ../../tslint.json src/**/*.ts test/**/*.ts",
    "tsc": "rimraf lib && tsc",
