{
  "name": "@pnpm/parse-cli-args",
  "version": "3.2.2",
  "description": "Parses the CLI args passed to pnpm",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib",
    "!*.map"
  ],
  "engines": {
    "node": ">=12.17"
  },
  "scripts": {
