{
  "name": "@pnpm/plugin-commands-listing",
  "version": "3.0.3",
  "description": "The list and why commands of pnpm",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib",
    "!*.map"
  ],
  "engines": {
    "node": ">=12.17"
  },
  "scripts": {
    "lint": "eslint -c ../../eslint.json src/**/*.ts test/**/*.ts",
    "registry-mock": "registry-mock",
    "test:jest": "jest",
    "test:e2e": "registry-mock prepare && run-p -r registry-mock test:jest",
    "_test": "cross-env PNPM_REGISTRY_MOCK_PORT=7774 pnpm run test:e2e",
    "test": "pnpm run compile && pnpm run _test",
    "prepublishOnly": "pnpm run compile",
    "compile": "rimraf lib tsconfig.tsbuildinfo && tsc --build && pnpm run lint -- --fix"
  },
  "repository": "https://github.com/pnpm/pnpm/blob/master/packages/plugin-commands-listing",
  "keywords": [
    "pnpm"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pnpm/pnpm/issues"
  },
  "homepage": "https://github.com/pnpm/pnpm/blob/master/packages/plugin-commands-listing#readme",
  "devDependencies": {
    "@pnpm/constants": "workspace:5.0.0",
    "@pnpm/filter-workspace-packages": "workspace:3.0.1",
    "@pnpm/logger": "^4.0.0",
    "@pnpm/plugin-commands-installation": "workspace:4.0.3",
    "@pnpm/prepare": "workspace:0.0.20",
    "@types/ramda": "^0.27.35",
    "execa": "^5.0.0",
    "strip-ansi": "^6.0.0",
    "write-yaml-file": "^4.2.0"
  },
  "dependencies": {
    "@pnpm/cli-utils": "workspace:0.6.1",
    "@pnpm/common-cli-options-help": "workspace:0.4.0",
    "@pnpm/config": "workspace:12.1.0",
    "@pnpm/error": "workspace:2.0.0",
    "@pnpm/list": "workspace:6.1.0",
    "@pnpm/types": "workspace:7.0.0",
    "ramda": "^0.27.1",
    "render-help": "^1.0.1"
  },
  "peerDependencies": {
    "@pnpm/logger": "^4.0.0"
  },
  "funding": "https://opencollective.com/pnpm"
}
