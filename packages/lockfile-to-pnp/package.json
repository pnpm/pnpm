{
  "name": "@pnpm/lockfile-to-pnp",
  "version": "0.4.6",
  "description": "Creates a Plug'n'Play file from a pnpm-lock.yaml",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "engines": {
    "node": ">=12.17"
  },
  "files": [
    "lib",
    "!*.map"
  ],
  "scripts": {
    "lint": "eslint -c ../../eslint.json src/**/*.ts test/**/*.ts",
    "test": "pnpm run compile && pnpm run _test",
    "prepublishOnly": "pnpm run compile",
    "_test": "jest",
    "compile": "rimraf lib tsconfig.tsbuildinfo && tsc --build && pnpm run lint -- --fix"
  },
  "repository": "https://github.com/pnpm/pnpm/blob/master/packages/lockfile-to-pnp",
  "keywords": [
    "pnpm",
    "shrinkwrap",
    "lockfile"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pnpm/pnpm/issues"
  },
  "homepage": "https://github.com/pnpm/pnpm/blob/master/packages/lockfile-to-pnp#readme",
  "peerDependencies": {
    "@pnpm/logger": "^4.0.0"
  },
  "devDependencies": {
    "@pnpm/logger": "^4.0.0",
    "@pnpm/types": "workspace:7.1.0",
    "@types/normalize-path": "^3.0.0",
    "@types/ramda": "0.27.39",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "@pnpm/config": "workspace:12.3.0",
    "@pnpm/lockfile-file": "workspace:4.0.1",
    "@pnpm/lockfile-utils": "workspace:3.0.3",
    "@pnpm/read-project-manifest": "workspace:2.0.2",
    "@yarnpkg/pnp": "^2.3.2",
    "dependency-path": "workspace:8.0.0",
    "normalize-path": "^3.0.0",
    "ramda": "^0.27.1"
  },
  "funding": "https://opencollective.com/pnpm"
}
