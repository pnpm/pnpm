{
  "name": "@pnpm/outdated",
  "version": "8.0.11",
  "description": "Check for outdated packages",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "engines": {
    "node": ">=12.17"
  },
  "files": [
    "lib",
    "!*.map"
  ],
  "scripts": {
    "test": "pnpm run compile && pnpm run _test",
    "lint": "eslint -c ../../eslint.json src/**/*.ts test/**/*.ts",
    "prepublishOnly": "pnpm run compile",
    "registry-mock": "registry-mock",
    "test:jest": "jest",
    "_test": "cross-env PNPM_REGISTRY_MOCK_PORT=7771 pnpm run test:e2e",
    "test:e2e": "registry-mock prepare && run-p -r registry-mock test:jest",
    "compile": "rimraf lib tsconfig.tsbuildinfo && tsc --build && pnpm run lint -- --fix"
  },
  "repository": "https://github.com/pnpm/pnpm/blob/master/packages/outdated",
  "keywords": [
    "pnpm",
    "outdated"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pnpm/pnpm/issues"
  },
  "homepage": "https://github.com/pnpm/pnpm/blob/master/packages/outdated#readme",
  "peerDependencies": {
    "@pnpm/logger": "^4.0.0"
  },
  "dependencies": {
    "@pnpm/client": "workspace:4.0.0",
    "@pnpm/constants": "workspace:5.0.0",
    "@pnpm/error": "workspace:2.0.0",
    "@pnpm/lockfile-file": "workspace:4.0.4",
    "@pnpm/lockfile-utils": "workspace:3.0.5",
    "@pnpm/manifest-utils": "workspace:2.0.2",
    "@pnpm/matcher": "workspace:2.0.0",
    "@pnpm/modules-yaml": "workspace:9.0.2",
    "@pnpm/pick-registry-for-package": "workspace:2.0.2",
    "@pnpm/store-path": "^5.0.0",
    "@pnpm/types": "workspace:7.2.0",
    "dependency-path": "workspace:8.0.1",
    "ramda": "^0.27.1",
    "semver": "^7.3.4"
  },
  "devDependencies": {
    "@pnpm/logger": "^4.0.0",
    "@types/ramda": "0.27.39",
    "@types/semver": "^7.3.4",
    "npm-run-all": "^4.1.5"
  },
  "funding": "https://opencollective.com/pnpm"
}
