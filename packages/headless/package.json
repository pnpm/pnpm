    "@pnpm/utils": "0.9.0",
    "@types/ramda": "0.25.34",
    "dependency-path": "2.0.0",
    "graph-sequencer": "2.0.0",
    "p-limit": "2.0.0",
    "package-store": "0.23.7",
    "path-exists": "3.0.0",
    "pnpm-shrinkwrap": "8.1.2",
    "ramda": "0.25.0",
    "symlink-dir": "2.0.2"
  }
