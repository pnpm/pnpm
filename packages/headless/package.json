    "rimraf": "2.6.2",
    "rimraf-then": "1.0.1",
    "sinon": "7.1.1",
    "tape": "4.9.1",
    "tape-promise": "4.0.0",
    "tempy": "0.2.1",
    "ts-node": "6.2.0",
    "tslint": "5.11.0",
    "tslint-config-standard": "8.0.1",
