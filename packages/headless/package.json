    "@types/path-exists": "3.0.0",
    "@types/rimraf": "2.0.2",
    "@types/sinon": "5.0.5",
    "@types/tape": "4.2.32",
    "@types/tempy": "0.2.0",
    "fs-extra": "7.0.1",
    "is-windows": "1.0.2",
    "isexe": "2.0.0",
    "mz": "2.7.0",
    "npm-run-all": "4.1.3",
