  "dependencies": {
    "@pnpm/core-loggers": "workspace:^4.2.0",
    "@pnpm/fetching-types": "workspace:^1.0.0",
    "@pnpm/npm-registry-agent": "workspace:^3.1.0",
    "@zkochan/retry": "^0.2.0",
    "node-fetch": "2.6.1",
    "node-fetch-unix": "2.3.0"
