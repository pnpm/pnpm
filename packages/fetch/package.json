  },
  "homepage": "https://github.com/pnpm/pnpm/blob/master/packages/fetch#readme",
  "dependencies": {
    "@zeit/fetch-retry": "5.0.0",
    "node-fetch": "2.6.0",
    "node-fetch-unix": "2.3.0"
  },
  "devDependencies": {
    "@types/node-fetch": "^2.5.5",
    "cpy-cli": "3.1.0"
