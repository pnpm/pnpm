{
  "name": "@pnpm/plugin-commands-nvm",
  "version": "0.1.0",
  "description": "pnpm commands for managing Node.js",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib",
    "!*.map"
  ],
  "engines": {
    "node": ">=12.17"
  },
  "scripts": {
    "lint": "eslint -c ../../eslint.json src/**/*.ts test/**/*.ts",
