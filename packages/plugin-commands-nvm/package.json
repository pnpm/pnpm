    "prepublishOnly": "pnpm run compile",
    "compile": "rimraf lib tsconfig.tsbuildinfo && tsc --build && pnpm run lint -- --fix"
  },
  "repository": "https://github.com/pnpm/pnpm/blob/master/packages/plugin-commands-nvm",
  "keywords": [
