{
  "name": "@pnpm/git-fetcher",
  "version": "2.0.2",
  "description": "A fetcher for git-hosted packages",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "test": "pnpm run tsc && pnpm run lint && ts-node test --type-check",
