  "dependencies": {
    "@types/node": "*",
    "dint": "4.0.0",
    "execa": "1.0.0",
    "path-temp": "1.0.0",
    "rimraf-then": "1.0.1"
  },
  "devDependencies": {
    "@pnpm/git-fetcher": "link:",
    "@pnpm/tslint-config": "0.0.0",
    "@types/tape": "^4.2.31",
