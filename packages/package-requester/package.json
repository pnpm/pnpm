{
  "name": "@pnpm/package-requester",
  "version": "9.1.0",
  "description": "Concurrent downloader of npm-compatible packages",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "pnpm run tsc -- --watch",
    "lint": "tslint -c tslint.json src/**/*.ts test/**/*.ts",
    "tsc": "rimraf lib && tsc --pretty",
    "test": "pnpm run lint && pnpm run tsc && rimraf .store && ts-node test --type-check",
    "prepublishOnly": "pnpm run tsc"
  },
  "repository": "https://github.com/pnpm/pnpm/blob/master/packages/package-requester",
  "keywords": [
    "pnpm",
    "resolver",
    "npm"
  ],
  "author": "Zoltan Kochan <z@kochan.io> (https://www.kochan.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pnpm/pnpm/issues"
  },
  "homepage": "https://github.com/pnpm/pnpm/blob/master/packages/package-requester#readme",
  "peerDependencies": {
    "@pnpm/logger": "^3.1.0"
  },
  "dependencies": {
    "@pnpm/check-package": "3.0.1",
    "@pnpm/core-loggers": "workspace:4.0.0",
    "@pnpm/fetcher-base": "workspace:5.1.0",
    "@pnpm/pkgid-to-filename": "2.0.0",
    "@pnpm/read-package-json": "workspace:3.0.0",
    "@pnpm/resolver-base": "workspace:5.1.0",
    "@pnpm/store-controller-types": "workspace:5.1.0",
    "@pnpm/types": "workspace:4.0.0",
    "@zkochan/rimraf": "1.0.0",
    "load-json-file": "6.2.0",
    "make-dir": "3.0.0",
    "mz": "2.7.0",
    "p-limit": "2.2.1",
    "p-queue": "6.2.1",
    "path-exists": "4.0.0",
    "promise-share": "1.0.0",
    "ramda": "0.26.1",
    "rename-overwrite": "2.0.2",
    "ssri": "6.0.1",
    "symlink-dir": "3.1.1",
    "write-json-file": "4.0.0"
  },
  "devDependencies": {
    "@pnpm/local-resolver": "workspace:*",
    "@pnpm/logger": "3.1.0",
    "@pnpm/npm-resolver": "workspace:*",
    "@pnpm/package-requester": "link:",
    "@pnpm/tarball-fetcher": "workspace:*",
    "@types/mz": "^2.7.0",
    "@types/ncp": "^2.0.3",
    "@types/normalize-path": "3.0.0",
    "@types/ramda": "^0.26.38",
    "@types/sinon": "^7.5.1",
    "@types/ssri": "6.0.1",
    "delay": "4.3.0",
    "ncp": "2.0.0",
    "nock": "11.7.0",
    "normalize-path": "3.0.0",
    "rimraf": "3.0.0",
    "sinon": "7.5.0",
    "tape": "4.11.0",
    "tempy": "0.3.0"
  }
}
