{
  "name": "@pnpm/package-requester",
  "version": "7.0.2",
  "description": "Concurrent downloader of npm-compatible packages",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "start": "npm run tsc -- --watch",
    "lint": "tslint -c tslint.json src/**/*.ts test/**/*.ts",
    "tsc": "rimraf lib && tsc --pretty",
    "test": "npm run lint && npm run tsc && rimraf .store && ts-node test --type-check && mos t",
    "md": "mos",
    "prepublishOnly": "npm run tsc"
  },
  "repository": "https://github.com/pnpm/pnpm/blob/master/packages/package-requester",
  "keywords": [
    "pnpm",
    "resolver",
    "npm"
  ],
  "author": "Zoltan Kochan <z@kochan.io> (https://www.kochan.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pnpm/pnpm/issues"
  },
  "homepage": "https://github.com/pnpm/pnpm/blob/master/packages/package-requester#readme",
  "peerDependencies": {
    "@pnpm/logger": "^2.1.0"
  },
  "dependencies": {
    "@pnpm/check-package": "3.0.0",
    "@pnpm/core-loggers": "3.0.0",
    "@pnpm/fetcher-base": "3.0.0",
    "@pnpm/pkgid-to-filename": "2.0.0",
    "@pnpm/read-package-json": "2.0.1",
    "@pnpm/resolver-base": "3.0.0",
    "@pnpm/store-controller-types": "3.0.0",
    "@pnpm/types": "3.0.0",
    "@types/mz": "0.0.32",
    "@types/node": "*",
    "load-json-file": "5.2.0",
    "mkdirp-promise": "5.0.1",
    "mz": "2.7.0",
    "p-limit": "2.2.0",
    "p-queue": "4.0.0",
    "path-exists": "4.0.0",
    "read-package-json": "2.0.13",
    "rename-overwrite": "1.0.3",
    "rimraf-then": "1.0.1",
    "symlink-dir": "3.0.1",
    "write-json-file": "3.1.0"
  },
  "devDependencies": {
    "@pnpm/local-resolver": "file:../local-resolver",
    "@pnpm/logger": "2.1.0",
    "@pnpm/npm-resolver": "file:../npm-resolver",
    "@pnpm/package-requester": "link:",
    "@pnpm/tarball-fetcher": "file:../tarball-fetcher",
    "@pnpm/tslint-config": "0.0.0",
    "@types/ncp": "2.0.1",
    "@types/nock": "9.3.1",
    "@types/sinon": "^7.0.10",
    "@types/tape": "4.2.33",
    "@types/tempy": "0.2.0",
    "delay": "4.1.0",
    "mos": "2.0.0-alpha.3",
    "mos-plugin-readme": "1.0.4",
    "ncp": "2.0.0",
    "nock": "10.0.6",
    "normalize-path": "3.0.0",
    "rimraf": "2.6.3",
    "sinon": "7.3.1",
    "tape": "4.10.1",
    "tempy": "0.2.1",
    "ts-node": "8.0.3",
    "tslint": "5.15.0",
    "typescript": "3.4.1"
  },
  "mos": {
    "plugins": [
      "readme"
    ],
    "installation": {
      "useShortAlias": true
    }
  },
  "publishConfig": {
    "scripts": {}
  }
}
