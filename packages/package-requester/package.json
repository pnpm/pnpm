{
  "name": "@pnpm/package-requester",
  "version": "19.0.3",
  "description": "Concurrent downloader of npm-compatible packages",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "!*.map"
  ],
  "engines": {
    "node": ">=14.6"
  },
  "scripts": {
    "start": "tsc --watch",
    "lint": "eslint src/**/*.ts test/**/*.ts",
    "_test": "cross-env PNPM_REGISTRY_MOCK_PORT=7772 pnpm run test:e2e",
    "test": "pnpm run compile && pnpm run _test",
    "prepublishOnly": "pnpm run compile",
    "compile": "tsc --build && pnpm run lint --fix",
    "registry-mock": "registry-mock",
    "test:jest": "jest",
    "test:e2e": "registry-mock prepare && run-p -r registry-mock test:jest"
  },
  "repository": "https://github.com/pnpm/pnpm/blob/main/packages/package-requester",
  "keywords": [
    "pnpm7",
    "pnpm",
    "resolver",
    "npm"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pnpm/pnpm/issues"
  },
  "homepage": "https://github.com/pnpm/pnpm/blob/main/packages/package-requester#readme",
  "peerDependencies": {
    "@pnpm/logger": "^4.0.0"
  },
  "dependencies": {
    "@pnpm/cafs": "workspace:*",
    "@pnpm/core-loggers": "workspace:*",
    "@pnpm/error": "workspace:*",
    "@pnpm/fetcher-base": "workspace:*",
    "@pnpm/graceful-fs": "workspace:*",
    "@pnpm/package-is-installable": "workspace:*",
    "@pnpm/pick-fetcher": "workspace:*",
    "@pnpm/read-package-json": "workspace:*",
    "@pnpm/resolver-base": "workspace:*",
    "@pnpm/store-controller-types": "workspace:*",
    "@pnpm/types": "workspace:*",
    "dependency-path": "workspace:*",
    "load-json-file": "^6.2.0",
    "p-defer": "^3.0.0",
    "p-limit": "^3.1.0",
    "p-queue": "^6.6.2",
    "path-temp": "^2.0.0",
    "promise-share": "^1.0.0",
    "ramda": "npm:@pnpm/ramda@0.28.1",
    "rename-overwrite": "^4.0.2",
    "safe-promise-defer": "^1.0.1",
    "semver": "^7.3.7",
    "ssri": "^9.0.1"
  },
  "devDependencies": {
    "@pnpm/cafs-types": "workspace:*",
    "@pnpm/client": "workspace:*",
    "@pnpm/create-cafs-store": "workspace:*",
    "@pnpm/logger": "^4.0.0",
    "@pnpm/package-requester": "workspace:*",
    "@pnpm/registry-mock": "3.0.0-3",
    "@pnpm/test-fixtures": "workspace:*",
    "@types/normalize-path": "^3.0.0",
    "@types/ramda": "0.28.15",
    "@types/semver": "7.3.10",
    "@types/ssri": "^7.1.1",
    "delay": "^5.0.0",
    "nock": "13.2.9",
    "normalize-path": "^3.0.0",
    "tempy": "^1.0.1"
  },
  "funding": "https://opencollective.com/pnpm",
  "exports": {
    ".": "./lib/index.js"
  }
}
