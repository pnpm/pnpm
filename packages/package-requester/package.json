    "ssri": "6.0.1"
  },
  "devDependencies": {
    "@pnpm/client": "workspace:2.0.19",
    "@pnpm/logger": "^3.2.3",
    "@types/mz": "^2.7.3",
    "@types/ncp": "^2.0.4",
    "@types/normalize-path": "^3.0.0",
    "@types/ramda": "^0.27.34",
    "@types/ssri": "^7.1.0",
    "delay": "^4.4.0",
