{
  "name": "@pnpm/symlink-dependency",
  "description": "Symlink a dependency to node_modules",
  "version": "3.0.10",
  "author": "Zoltan Kochan <z@kochan.io> (https://www.kochan.io/)",
  "bugs": {
    "url": "https://github.com/pnpm/pnpm/issues"
  },
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib",
    "!*.map"
  ],
  "peerDependencies": {
    "@pnpm/logger": "^3.1.0"
  },
  "devDependencies": {
    "@pnpm/logger": "^3.2.2",
    "@types/mz": "^2.7.1"
  },
  "directories": {
    "test": "test"
  },
  "homepage": "https://github.com/pnpm/pnpm/blob/master/packages/symlink-dependency#readme",
  "keywords": [
    "pnpm"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=10.14"
  },
  "repository": "https://github.com/pnpm/pnpm/blob/master/packages/symlink-dependency",
  "scripts": {
    "start": "pnpm run tsc -- --watch",
    "test": "pnpm run compile",
    "lint": "eslint -c ../../eslint.json src/**/*.ts",
    "prepublishOnly": "pnpm run compile",
    "compile": "rimraf lib tsconfig.tsbuildinfo && tsc --build"
  },
  "dependencies": {
    "@pnpm/core-loggers": "workspace:5.0.0",
    "@pnpm/types": "workspace:^6.2.0",
    "mz": "^2.7.0",
    "symlink-dir": "^4.1.0"
  },
  "funding": "https://opencollective.com/pnpm"
}
