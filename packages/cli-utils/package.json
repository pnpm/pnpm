    "@types/ramda": "0.27.39"
  },
  "dependencies": {
    "@pnpm/cli-meta": "workspace:2.0.0",
    "@pnpm/config": "workspace:12.6.0",
    "@pnpm/default-reporter": "workspace:8.2.2",
    "@pnpm/error": "workspace:2.0.0",
    "@pnpm/manifest-utils": "workspace:2.1.0",
