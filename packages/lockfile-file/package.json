{
  "name": "@pnpm/lockfile-file",
  "version": "5.3.8",
  "description": "Read/write pnpm-lock.yaml files",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "engines": {
    "node": ">=14.6"
  },
  "files": [
    "lib",
    "!*.map"
  ],
  "scripts": {
