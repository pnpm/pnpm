  "version": "0.1.7",
  "bugs": {
    "url": "https://github.com/pnpm/pnpm/issues"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "!*.map"
  ],
  "homepage": "https://github.com/pnpm/pnpm/blob/main/packages/real-hoist#readme",
  "keywords": [
    "pnpm7",
    "pnpm"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=12.17"
  },
  "repository": "https://github.com/pnpm/pnpm/blob/main/packages/real-hoist",
  "scripts": {
