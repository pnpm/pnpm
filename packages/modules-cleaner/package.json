    "@pnpm/lockfile-utils": "workspace:3.1.0",
    "@pnpm/read-modules-dir": "workspace:3.0.1",
    "@pnpm/remove-bins": "workspace:2.0.7",
    "@pnpm/store-controller-types": "workspace:11.0.6",
    "@pnpm/types": "workspace:7.5.0",
    "@zkochan/rimraf": "^2.1.1",
    "dependency-path": "workspace:8.0.5",
    "ramda": "^0.27.1"
  },
  "devDependencies": {
    "@pnpm/logger": "^4.0.0",
    "@types/ramda": "0.27.39"
  },
  "bugs": {
    "url": "https://github.com/pnpm/pnpm/issues"
  },
  "funding": "https://opencollective.com/pnpm"
