  "directories": {
    "test": "test"
  },
  "homepage": "https://pnpm.js.org",
  "keywords": [
    "dependency manager",
    "install",
    "installer",
    "uninstall",
    "remove",
    "link",
    "prune",
    "shrinkwrap",
    "lockfile",
    "fast",
    "rapid",
    "efficient",
    "package.json",
    "packages",
    "dependencies",
    "symlinks",
    "hardlinks",
    "modules",
    "npm",
    "package manager",
    "monorepo",
    "multi-package",
    "workspace"
