  "repository": {
    "type": "git",
    "url": "git+https://github.com/pnpm/pnpm.git"
  },
  "scripts": {
    "start": "npm run tsc -- --watch",
    "?commit": "echo 'Run Git commit wizard'",
    "commit": "commit",
    "commitmsg": "commitlint -e",
    "lint": "tslint -c tslint.json src/**/*.ts test/**/*.ts",
    "registry-mock": "registry-mock",
    "test:tap": "ts-node test --type-check",
    "pretest:e2e": "rimraf node_modules/.bin/pnpm && registry-mock prepare",
    "test:e2e": "run-p -r registry-mock test:tap",
    "test": "npm run lint && npm run tsc && cross-env PNPM_REGISTRY_MOCK_PORT=7772 npm run test:e2e",
    "tsc": "rimraf lib && tsc",
    "release": "sh .scripts/release.sh"
  },
  "publishconfig": {
    "tag": "next"
  }
}
