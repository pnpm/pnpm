    "@pnpm/prepare": "workspace:*",
    "@pnpm/registry-mock": "2.20.0",
    "@pnpm/test-fixtures": "workspace:*",
    "@types/ramda": "0.28.14",
    "@types/semver": "^7.3.10",
    "@types/sinon": "^10.0.13",
    "execa": "npm:safe-execa@^0.1.2",
