    "mem": "^8.1.1",
    "p-limit": "^3.1.0",
    "ramda": "npm:@pnpm/ramda@0.28.1",
    "read-ini-file": "^3.1.0",
    "render-help": "^1.0.2",
    "run-groups": "^3.0.1",
    "semver": "^7.3.7"
  },
  "peerDependencies": {
    "@pnpm/logger": "^4.0.0"
  },
  "funding": "https://opencollective.com/pnpm",
