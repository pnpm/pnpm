{
  "name": "@pnpm/make-dedicated-lockfile",
  "version": "0.2.1",
  "description": "Creates a dedicated lockfile for a subset of workspace projects",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "bin": "./bin/make-dedicated-lockfile.js",
  "files": [
    "lib",
    "!*.map",
    "bin"
  ],
  "engines": {
    "node": ">=12.17"
  },
  "scripts": {
    "lint": "eslint -c ../../eslint.json src/**/*.ts test/**/*.ts",
    "_test": "jest",
    "test": "pnpm run compile && pnpm run _test",
    "prepublishOnly": "pnpm run compile",
    "compile": "rimraf lib tsconfig.tsbuildinfo && tsc --build && pnpm run lint -- --fix"
  },
  "repository": "https://github.com/pnpm/pnpm/blob/master/packages/make-dedicated-lockfile",
  "keywords": [
    "pnpm",
    "make-dedicated-lockfile"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pnpm/pnpm/issues"
  },
  "homepage": "https://github.com/pnpm/pnpm/blob/master/packages/make-dedicated-lockfile#readme",
  "dependencies": {
    "@pnpm/error": "workspace:2.0.0",
    "@pnpm/exec": "^2.0.0",
    "@pnpm/exportable-manifest": "workspace:2.0.0",
    "@pnpm/find-workspace-dir": "workspace:3.0.0",
    "@pnpm/lockfile-file": "workspace:4.0.0",
    "@pnpm/logger": "^4.0.0",
    "@pnpm/prune-lockfile": "workspace:3.0.1",
    "@pnpm/read-project-manifest": "workspace:2.0.0",
    "@pnpm/types": "workspace:7.0.0",
    "ramda": "^0.27.1",
    "rename-overwrite": "^3.1.0"
  },
  "funding": "https://opencollective.com/pnpm",
  "devDependencies": {
    "@pnpm/prepare": "workspace:0.0.19",
    "@types/ncp": "^2.0.4",
    "@types/ramda": "^0.27.35",
    "ncp": "^2.0.0"
  }
}
