    "@pnpm/lifecycle": "workspace:9.6.2",
    "@pnpm/link-bins": "workspace:5.3.20",
    "@pnpm/read-package-json": "workspace:3.1.8",
    "@pnpm/store-controller-types": "workspace:9.2.0",
    "@pnpm/types": "workspace:6.3.1",
