{
  "name": "@pnpm/lockfile-utils",
  "version": "4.2.4",
  "description": "Utils for dealing with pnpm-lock.yaml",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "engines": {
    "node": ">=14.6"
  },
  "files": [
    "lib",
    "!*.map"
  ],
  "scripts": {
