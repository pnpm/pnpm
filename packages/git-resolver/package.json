{
  "name": "@pnpm/git-resolver",
  "version": "5.1.1",
  "description": "Resolver for git-hosted packages",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib",
    "!*.map"
  ],
  "engines": {
    "node": ">=12.17"
  },
  "scripts": {
    "lint": "eslint -c ../../eslint.json src/**/*.ts test/**/*.ts",
    "_test": "jest",
    "test": "pnpm run compile && pnpm run _test",
    "prepublishOnly": "pnpm run compile",
    "fix": "tslint -c tslint.json src/**/*.ts test/**/*.ts --fix",
    "compile": "rimraf lib tsconfig.tsbuildinfo && tsc --build && pnpm run lint -- --fix"
  },
  "repository": "https://github.com/pnpm/pnpm/blob/master/packages/git-resolver",
  "keywords": [
    "pnpm",
    "resolver",
    "npm"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pnpm/pnpm/issues"
  },
  "homepage": "https://github.com/pnpm/pnpm/blob/master/packages/git-resolver#readme",
  "dependencies": {
    "@pnpm/fetch": "workspace:4.0.1",
    "@pnpm/resolver-base": "workspace:8.0.3",
    "graceful-git": "^3.0.2",
    "hosted-git-info": "^4.0.1",
    "semver": "^7.3.4"
  },
  "devDependencies": {
    "@types/hosted-git-info": "^3.0.1",
    "@types/is-windows": "^1.0.0",
    "@types/semver": "^7.3.4",
    "is-windows": "^1.0.2"
  },
  "funding": "https://opencollective.com/pnpm"
}
