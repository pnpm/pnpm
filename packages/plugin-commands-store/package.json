{
  "name": "@pnpm/plugin-commands-store",
  "version": "3.0.3",
  "description": "Commands for controlling the store",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib",
    "!*.map"
  ],
  "engines": {
    "node": ">=12.17"
  },
  "scripts": {
    "lint": "eslint -c ../../eslint.json src/**/*.ts test/**/*.ts",
    "registry-mock": "registry-mock",
    "test:jest": "jest",
    "test:e2e": "registry-mock prepare && run-p -r registry-mock test:jest",
    "_test": "cross-env PNPM_REGISTRY_MOCK_PORT=7779 pnpm run test:e2e",
    "test": "pnpm run compile && pnpm run _test",
    "prepublishOnly": "pnpm run compile",
    "compile": "rimraf lib tsconfig.tsbuildinfo && tsc --build && pnpm run lint -- --fix"
  },
  "repository": "https://github.com/pnpm/pnpm/blob/master/packages/plugin-commands-store",
  "keywords": [
    "pnpm",
    "scripts"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pnpm/pnpm/issues"
  },
  "homepage": "https://github.com/pnpm/pnpm/blob/master/packages/plugin-commands-store#readme",
  "devDependencies": {
    "@pnpm/assert-store": "workspace:*",
    "@pnpm/lockfile-file": "workspace:4.0.0",
    "@pnpm/logger": "^4.0.0",
    "@pnpm/prepare": "workspace:0.0.20",
    "@types/archy": "0.0.31",
    "@types/ramda": "^0.27.35",
    "@types/sinon": "^9.0.11",
    "@types/ssri": "^7.1.0",
    "@zkochan/rimraf": "^2.1.1",
    "execa": "^5.0.0",
    "load-json-file": "^6.2.0",
    "path-exists": "^4.0.0",
    "sinon": "^9.2.2",
    "ssri": "^8.0.1",
    "tempy": "^1.0.0"
  },
  "dependencies": {
    "@pnpm/cafs": "workspace:3.0.1",
    "@pnpm/cli-utils": "workspace:0.6.1",
    "@pnpm/config": "workspace:12.1.0",
    "@pnpm/error": "workspace:2.0.0",
    "@pnpm/get-context": "workspace:5.0.0",
    "@pnpm/lockfile-utils": "workspace:3.0.1",
    "@pnpm/normalize-registries": "workspace:2.0.0",
    "@pnpm/parse-wanted-dependency": "workspace:2.0.0",
    "@pnpm/pick-registry-for-package": "workspace:2.0.0",
    "@pnpm/store-connection-manager": "workspace:2.0.3",
    "@pnpm/store-controller-types": "workspace:11.0.0",
    "@pnpm/store-path": "^5.0.0",
    "@pnpm/types": "workspace:7.0.0",
    "archy": "^1.0.0",
    "dependency-path": "workspace:7.0.0",
    "dint": "^5.1.0",
    "p-filter": "^2.1.0",
    "ramda": "^0.27.1",
    "render-help": "^1.0.1"
  },
  "peerDependencies": {
    "@pnpm/logger": "^4.0.0"
  },
  "funding": "https://opencollective.com/pnpm"
}
