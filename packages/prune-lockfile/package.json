{
  "name": "@pnpm/prune-lockfile",
  "version": "2.0.6",
  "description": "Prune a pnpm-lock.yaml",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "engines": {
    "node": ">=10"
  },
  "files": [
    "lib/"
  ],
  "scripts": {
    "lint": "tslint -c ../../tslint.json src/**/*.ts test/**/*.ts",
