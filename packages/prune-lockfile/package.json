  "devDependencies": {
    "@pnpm/prune-lockfile": "workspace:3.0.14",
    "@types/ramda": "0.27.39",
    "yaml-tag": "1.1.0"
  },
  "dependencies": {
    "@pnpm/constants": "workspace:5.0.0",
    "@pnpm/lockfile-types": "workspace:3.1.5",
    "@pnpm/types": "workspace:7.9.0",
