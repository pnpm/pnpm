{
  "name": "dependencies-hierarchy",
  "version": "3.1.0-0",
  "description": "Creates a dependencies hierarchy for a symlinked `node_modules`",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "fix": "tslint -c tslint.json --project . --fix",
    "lint": "tslint -c tslint.json --project .",
    "test": "npm run tsc && npm run lint && pnpm link . && ts-node test --type-check && mos t",
    "prepublishOnly": "tsc",
    "tsc": "tsc",
    "md": "pnpm link . && tsc && mos"
  },
  "engines": {
    "node": ">=6"
  },
  "repository": "https://github.com/pnpm/pnpm/blob/master/packages/dependencies-hierarchy",
  "keywords": [
    "hierarchy",
    "pnpm",
    "dependencies",
    "node_modules"
  ],
  "author": {
    "name": "Zoltan Kochan",
    "email": "z@kochan.io",
    "url": "https://www.kochan.io/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pnpm/pnpm/issues"
  },
  "homepage": "https://github.com/pnpm/pnpm/blob/master/packages/dependencies-hierarchy#readme",
  "dependencies": {
    "@types/node": "^9.3.0 || 10",
    "@types/semver": "^5.3.32",
    "dependency-path": "2.0.0",
    "pnpm-shrinkwrap": "8.0.0-0",
    "semver": "5.6.0"
  },
  "devDependencies": {
    "@pnpm/logger": "^2.1.0",
    "@types/tape": "^4.2.30",
    "mos": "^2.0.0-alpha.3",
    "mos-plugin-readme": "^1.0.4",
    "tape": "^4.7.0",
    "ts-node": "^7.0.0",
    "tslint": "^5.8.0",
    "tslint-config-standard": "8.0.1",
    "tslint-eslint-rules": "5.4.0",
    "typescript": "^3.0.0"
  },
  "mos": {
    "plugins": [
      "readme"
    ],
    "installation": {
      "useShortAlias": true
    }
  }
}
