    "@pnpm/error": "workspace:^",
    "@pnpm/find-workspace-dir": "workspace:^",
    "@pnpm/nopt": "catalog:",
    "@pnpm/parse-cli-args": "workspace:^",
    "@pnpm/tabtab": "catalog:",
