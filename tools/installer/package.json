{
  "name": "@pnpm/tools.installer",
  "version": "1000.0.0",
  "description": "Generic tool installer for pnpm",
  "keywords": [
    "pnpm",
    "pnpm11",
    "installer"
  ],
  "license": "MIT",
  "funding": "https://opencollective.com/pnpm",
  "repository": "https://github.com/pnpm/pnpm/tree/main/tools/installer",
  "homepage": "https://github.com/pnpm/pnpm/tree/main/tools/installer#readme",
  "bugs": {
    "url": "https://github.com/pnpm/pnpm/issues"
  },
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": "./lib/index.js"
  },
  "files": [
    "lib",
    "!*.map"
  ],
  "scripts": {
    "lint": "eslint \"src/**/*.ts\"",
    "prepublishOnly": "pnpm run compile",
    "compile": "tsc --build && pnpm run lint --fix",
    "test": "pnpm run compile"
  },
  "dependencies": {
    "@pnpm/exec.pnpm-cli-runner": "workspace:*",
    "@pnpm/tools.path": "workspace:*",
    "@zkochan/rimraf": "catalog:",
    "path-temp": "catalog:",
    "rename-overwrite": "catalog:"
  },
  "devDependencies": {
    "@pnpm/tools.installer": "workspace:*"
  },
  "engines": {
    "node": ">=20.19"
  },
  "jest": {
    "preset": "@pnpm/jest-config"
  }
}
